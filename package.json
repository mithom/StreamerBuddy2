{
  "name": "vite-electron-builder",
  "version": "0.0.1",
  "private": true,
  "main": "dist/source/main/background.js",
  "scripts": {
    "buildEnvTypes": "node ./bin/buildEnvTypes.js",
    "prebuild": "npm run buildEnvTypes",
    "build": "node ./bin/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config ./config/electron-builder.js",
    "pretest": "cross-env MODE=test npm run build",
    "test": "node ./tests/app.spec.js",
    "prewatch": "npm run buildEnvTypes",
    "watch": "node ./bin/watch.js"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-window-state": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.3",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.18.0",
    "eslint-plugin-vue": "^7.5.0",
    "postcss": "^7.0.35",
    "slash": "^3.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.48"
  },
  "dependencies": {
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
